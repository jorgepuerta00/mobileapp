{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-mdpi/ && rm -rf ./android/app/src/main/res/raw/",
    "release": "yarn build && cd ./android && ./gradlew bundleRelease",
    "remove-duplicate-files": "rm -rf drawable*/node*",
    "release-apk": "cd android && yarn remove-duplicate-files  && ./gradlew assembleRelease",
    "android-build-release": "cd ./android && rm -rf app/src/main/res/drawable* && rm -rf ./android/app/src/main/res/raw/ && ./gradlew app:assembleRelease"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/datetimepicker": "2.4.0",
    "@react-native-community/masked-view": "0.1.10",
